{
  "name": "dvops-part1",
  "version": "1.0.0",
  "description": "This is a repository for the DVOPS Project",
  "main": "index.js",
  "nyc": {
    "include": [
      "utils/UserUtil.js",
      "tests/userTests.test.js",
      "models/User.js",
      "utils/users.json",
      "utils/EndOfDay.js",
      "utils/GetProfile.js",
      "utils/EditProfile.js",
      "tests/deleteUser.test.js",
      "tests/timeIn.test.js",
      "tests/EdwardUITests.test.js",
      "public/js/login.js",
      "public/js/register.js"

    ],
    "exclude": [],
    "reporter": [
      "html", "text"
    ],
    "all": true,
    "check-coverage": true,
    "branches": 80,
    "lines": 80,
    "functions": 80,
    "statements": 80
  },
  "scripts": {

    "test": "nyc mocha tests/*.test.js",
    "start": "node index.js",
    "frontend-instrument": "nyc instrument .\\public\\js .\\public\\instrumented\\js &&copy .\\public\\*.html .\\public\\instrumented\\ && copy .\\public\\images.\\public\\instrumented\\ && copy .\\public\\css .\\public\\instrumented\\ && del.\\coverage-frontend\\*.json",
    "frontend-report": "nyc report --reporter html --reporter text -t coverage-frontend--report-dir coverage-frontend/summary"

  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.20.2",
    "chai": "^4.3.10",
    "chromedriver": "^120.0.2",
    "date-fns": "^3.3.0",
    "express": "^4.18.2",
    "fs": "^0.0.1-security",
    "mocha": "^10.2.0",
    "ncp": "^2.0.0",
    "nyc": "^15.1.0",
    "selenium-webdriver": "^4.17.0"
  }
}
